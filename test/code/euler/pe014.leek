let N = 1000000
var cache = []
cache.fill(0, N)
cache[1] = 1

let sequence_length = function(n) {
	if (n >= N) {
		return if n % 2 {
			1 + Number.long(sequence_length(n * 3 + 1))
		} else {
			1 + Number.long(sequence_length(n / 2))
		}
	}
	var c = cache[n]
	if c return c

	return cache[n] = if n % 2 {
		1 + Number.long(sequence_length(n * 3 + 1))
	} else {
		1 + Number.long(sequence_length(n / 2))
	}
}

var max = 0
var n = 0

for var i = 1l; i < 1000000; i++Â {
	var l = sequence_length(i)
	if l > max {
		max = l
		n = i
	}
}

