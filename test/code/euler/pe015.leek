let S = 20

var paths = [0] // TODO var paths = new Array<int>(20) ?
paths.fill(0, 21) // TODO fill gives the array his element type

function walk(x, y) {
	if (x + y == S) {
		paths[y]++
		return
	}
	if (x > 0) walk(x - 1, y)
	if (y > 0) walk(x, y - 1)
	return // TODO useless return here, but prevents a leak
}

walk(S, S)

var sum = 1l
for (var i = 0; i < S; ++i) {
	sum += paths[i] * paths[i] // TODO x ** 2 optimization into x * x
}
return sum
